{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**/*"]
		},
		"test": {
			"dependsOn": ["^test"],
			"outputs": ["coverage/**/*"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check": {
			"dependsOn": ["typecheck", "lint", "test"]
		}
	}
}
